import javax.swing.*;
import java.awt.*;

public class ConversionCalculator extends JFrame{
	
	private final int WINDOW_WIDTH = 1000;
	private final int WINDOW_HEIGTH = 400;
	
	
	public ConversionCalculator(){
		
		JFrame frame = new JFrame("Conversion Calculator");
		frame.setDefaultCloseOperation(frame.EXIT_ON_CLOSE);
		frame.setVisible(true);
		//Window opens with top left corner at (500,500)
		frame.setBounds(500, 500, WINDOW_WIDTH, WINDOW_HEIGTH);
		
		
	}
	
	
}
